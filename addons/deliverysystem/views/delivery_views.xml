<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="deliverysystem.delivery_partner_form" model="ir.ui.view">
    <field name="name">delivery.partner.form</field>
    <field name="model">delivery.partner</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name" />
            <field name="contact_info" />
            <field name="status" />
          </group>
        </sheet>
      </form>
    </field>

  </record>
  <record id="deliverysystem.delivery_partner_action" model="ir.actions.act_window">
    <field name="name">Partners</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">delivery.partner</field>
    <field name="view_mode">list,form</field>
  </record>

  <menuitem id="deliverysystem.delivery_root"
    name="Delivery System"
    sequence="10" />
  <menuitem id="deliverysystem.delivery_partner_menu"
    name="Delivery Partners"
    action="deliverysystem.delivery_partner_action"
    parent="deliverysystem.delivery_root"
  />
  <menuitem
    id="deliverysystem.test"
    name="test"
    parent="deliverysystem.delivery_root"
  />


</odoo>