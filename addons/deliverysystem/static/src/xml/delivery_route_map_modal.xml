<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="deliverysystem.OpenRouteMapModal">
        <Dialog title="'Delivery Route Map'" size="'xl'" onClose="closeModal">
            <div class="o_delivery_route_map_container" style="height: 500px; width: 100%;">
                <DeliveryRouteMap 
                    routeId="state.routeId"
                    stops="state.stops"
                    apiKey="state.apiKey"
                />
            </div>
        </Dialog>
    </t>
</templates>